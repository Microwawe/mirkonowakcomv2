		"eslint": "^7.29.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-nuxt": "^2.0.0",
		"eslint-plugin-vue": "^7.12.1",
		"husky": "^6.0.0",
		"lint-staged": "^10.5.4",
		"postcss": "^8.3.5",
		"prettier": "^2.3.2",
